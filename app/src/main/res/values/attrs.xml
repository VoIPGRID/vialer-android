<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DialpadButton">
        <attr name="digit" format="string" />
        <attr name="chars" format="string" />
    </declare-styleable>

    <declare-styleable name="TwoStepCallIconView">
        <attr name="background_color" format="color" />
        <attr name="foreground_color" format="color" />
        <attr name="icon_resource" format="integer" />
    </declare-styleable>

    <declare-styleable name="Dialer">
        <attr name="show_exit_button" format="boolean" />
        <attr name="show_remove_button" format="boolean" />
        <attr name="show_call_button" format="boolean" />
        <attr name="keypad_height_percentage" format="float"/>
    </declare-styleable>
</resources>
