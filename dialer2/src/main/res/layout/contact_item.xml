<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:padding="0.0dip"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/timeline_item_bg_selector"
    xmlns:android="http://schemas.android.com/apk/res/android">
  <LinearLayout 
      android:padding="0.0dip" 
      android:layout_width="fill_parent" 
      android:layout_height="wrap_content">
    <LinearLayout 
        android:orientation="horizontal" 
        android:paddingLeft="10.0dip" 
        android:paddingTop="10.0dip" 
        android:paddingBottom="10.0dip" 
        android:layout_width="wrap_content" 
        android:layout_height="wrap_content" 
        android:layout_weight="1.0">
      <RelativeLayout android:layout_width="55.0dip" android:layout_height="48.0dip" android:layout_marginRight="3.0dip">
	<ImageView android:id="@+id/text_view_contact_icon"
	android:layout_width="48.0dip" android:layout_height="48.0dip"
	android:layout_alignParentLeft="true"
	android:layout_alignParentTop="true" android:layout_alignParentBottom="true"
	android:src="@drawable/default_icon" android:scaleType="fitXY" />
	<ImageView android:id="@+id/ContactOverlayImage"
	android:layout_width="20.0dip" android:layout_height="13.0dip"
	android:layout_alignParentRight="true" android:layout_alignParentBottom="true"
	android:src="@drawable/overlay_outgoing" android:visibility="gone" android:scaleType="fitXY" />
      </RelativeLayout>
      <LinearLayout 
          android:orientation="vertical" 
          android:layout_width="wrap_content" 
          android:layout_height="wrap_content" 
          android:layout_marginBottom="2.0dip" 
          android:layout_weight="1.0">
        <LinearLayout 
            android:gravity="center_vertical" 
            android:orientation="horizontal" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content">
          <TextView 
              android:id="@+id/text_view_contact_name"
              android:textSize="16.0dip" 
              android:textStyle="bold" 
              android:textColor="@color/text_main" 
              android:layout_width="wrap_content" 
              android:layout_height="wrap_content" 
              android:text="Contact Name" />
          <TextView
          	  android:id="@+id/ContactCallCount"
          	  android:textSize="12.0dip"
          	  android:textColor="@color/text_sub"
          	  android:layout_width="wrap_content"
          	  android:layout_height="wrap_content"
        	  android:layout_marginLeft="4.0dip"
          	  android:text="(??)"
          	  />
        </LinearLayout>
        <TextView 
            android:id="@+id/text_view_contact_phone_number"
            android:textSize="14.0dip" 
            android:textColor="@color/text_main" 
            android:textColorLink="@color/text_link" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content" 
            android:text="Test Test Test Test test test test test test test test" />
        <TextView 
            android:id="@+id/ContactLastDialed" 
            android:textSize="10.0dip" 
            android:textColor="@color/text_sub" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content" 
            android:text="00:00 00/00" />
      </LinearLayout>
    </LinearLayout>
    <ImageView 
		      android:id="@+id/ContactStarred"
		      android:paddingLeft="5.0dip"
		      android:paddingTop="10.0dip"
		      android:paddingRight="5.0dip"
		      android:visibility="gone" 
		      android:layout_width="wrap_content" 
		      android:layout_height="wrap_content" 
		      android:layout_marginBottom="5.0dip" 
		      android:src="@drawable/icon_starred" />
</LinearLayout>
</LinearLayout>